{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText1",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "AzureSqlTable2",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "cast1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          EEID as string,",
				"          {Full Name} as string,",
				"          {Job Title} as string,",
				"          Department as string,",
				"          {Business Unit} as string,",
				"          Gender as string,",
				"          Ethnicity as string,",
				"          Age as string,",
				"          {Hire Date} as string,",
				"          {Annual Salary} as string,",
				"          {Bonus %} as string,",
				"          Country as string,",
				"          City as string,",
				"          {Exit Date} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 derive({Annual Salary} = replace({Annual Salary},'$',''),",
				"          {Bonus %} = replace({Bonus %},'%','')) ~> derivedColumn1",
				"derivedColumn1 cast(output(",
				"          {Hire Date} as date 'MM/dd/yyyy',",
				"          {Annual Salary} as decimal(10,0) '###,###,000.###'",
				"     ),",
				"     errors: true) ~> cast1",
				"cast1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          EEID as string,",
				"          FullName as string,",
				"          JobTitle as string,",
				"          Department as string,",
				"          {Business Unit} as string,",
				"          Gender as string,",
				"          Ethnicity as string,",
				"          Age as integer,",
				"          {Hire Date} as date,",
				"          {Annual Salary} as decimal(19,4),",
				"          {Bonus %} as integer,",
				"          Country as string,",
				"          City as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          EEID,",
				"          Department,",
				"          {Business Unit},",
				"          Gender,",
				"          Ethnicity,",
				"          {Annual Salary},",
				"          Country,",
				"          City,",
				"          FullName = {Full Name},",
				"          JobTitle = {Job Title},",
				"          {Hire Date}",
				"     )) ~> sink1"
			]
		}
	}
}