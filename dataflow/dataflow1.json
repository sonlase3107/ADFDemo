{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText1",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          EEID as string,",
				"          {Full Name} as string,",
				"          {Job Title} as string,",
				"          Department as string,",
				"          {Business Unit} as string,",
				"          Gender as string,",
				"          Ethnicity as string,",
				"          Age as string,",
				"          {Hire Date} as date,",
				"          {Annual Salary} as string,",
				"          {Bonus %} as string,",
				"          Country as string,",
				"          City as string,",
				"          {Exit Date} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     ignoreNoFilesFound: false) ~> source1"
			]
		}
	}
}